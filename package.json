{
  "name": "@agorize/agorize-uikit",
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Agorize/agorize-uikit.git"
  },
  "scripts": {
    "build": "rollup -c",
    "release": "git-cz",
    "semantic-release": "semantic-release",
    "styleguide": "vue-styleguidist server",
    "styleguide:build": "vue-styleguidist build",
    "test": "jest"
  },
  "version": "0.0.0-alpha.1",
  "author": "",

  "keywords": [],
  "license": "ISC",
  "main": "dist/index.min.js",
  "bugs": {
    "url": "https://github.com/Agorize/agorize-uikit/issues"
  },
  "bundleDependencies": false,
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "deprecated": false,
  "description": "",
  "devDependencies": {
    "@semantic-release/commit-analyzer": "^6.1.0",
    "@semantic-release/github": "^5.2.0",
    "@semantic-release/npm": "^5.0.5",
    "@semantic-release/release-notes-generator": "^7.1.1",
    "@vue/test-utils": "^1.0.0-beta.25",
    "autoprefixer": "^9.2.1",
    "commitizen": "^3.0.4",
    "cz-conventional-changelog": "2.1.0",
    "fs-extra": "^7.0.0",
    "inquirer": "^6.2.0",
    "jest": "^23.6.0",
    "jest-transform-css": "^2.0.0",
    "lodash": "^4.17.11",
    "node-sass": "^4.9.4",
    "postcss": "^7.0.5",
    "rollup": "^0.66.6",
    "rollup-plugin-buble": "^0.19.4",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-sass": "^0.9.2",
    "rollup-plugin-scss": "^0.4.0",
    "rollup-plugin-uglify": "^6.0.0",
    "rollup-plugin-vue": "^4.3.2",
    "semantic-release": "^15.10.5",
    "vue": "^2.5.17",
    "vue-jest": "^3.0.0",
    "vue-styleguidist": "^1.8.9",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.20.2"
  },
  "homepage": "https://github.com/Agorize/agorize-uikit#readme",
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".+\\.(css|styl|less|sass|scss)$": "jest-transform-css"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ]
  }
}
